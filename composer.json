{
    "name": "adquesto/adquesto-php-sdk",
    "description": "Adquesto PHP integration toolkit",
    "type": "library",
    "require": {
        "sunra/php-simple-html-dom-parser": "v1.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Adquesto\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adquesto\\SDK\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-curl": "Needed to support CurlHttpClient implementation"
    }
}
